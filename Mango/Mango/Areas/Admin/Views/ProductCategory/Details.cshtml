@using Mango.Areas.Admin.ViewModels.ProductCategory
@model ProductCategoryDetailsVM


<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Product Category</h4>
        </div>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Title</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Title</td>
                    </tr>
                </tbody>
            </table>
            <a asp-action="index" asp-controller="productcategory" class="btn btn-info">Cancel</a>
        </div>
    </div>
</div>