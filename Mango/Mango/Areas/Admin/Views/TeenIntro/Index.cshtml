@using Mango.Areas.Admin.ViewModels.TeenIntro
@model TeenIntroIndexVm


<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Teen Intro</h4>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Title</th>
                                        <th>Description</th>
                                        <th>Url</th>
                                        <th>Photo</th>
                                        <th>Actions</th>

                                    </tr>
                                </thead>
                                <tbody>

                                    @{
                                        @if (Model.TeenIntro == null)
                                        {

                                            <a class="btn btn-primary" asp-action="create" asp-controller="teenintro" asp-area="admin">CREATE Teen Intro</a>

                                        }
                                        else
                                        {
                                            <tr>
                                                <td>@Model.TeenIntro.Id</td>
                                                <td>@Model.TeenIntro.Title</td>
                                                <td>@Model.TeenIntro.Description</td>
                                                <td>@Model.TeenIntro.Url</td>
                                                <td><img src="~/assets/img/@Model.TeenIntro.PhotoPath" /></td>
                                                <td>
                                                    <a class="btn btn-primary" asp-action="update" asp-controller="teenintro" asp-area="admin" asp-route-id="@Model.TeenIntro.Id">Update</a>
                                                    <a class="btn btn-danger" onclick="confirm('Eminsinizmi?')" asp-action="delete" asp-controller="teenintro" asp-area="admin" asp-route-id="@Model.TeenIntro.Id">Delete</a>
                                                    <a class="btn btn-info" asp-action="details" asp-controller="teenintro" asp-area="admin" asp-route-id="@Model.TeenIntro.Id">Details</a>
                                                </td>
                                            </tr>
                                        }
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

